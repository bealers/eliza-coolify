  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bealers/elizify"
  },
  "dependencies": {
    "@elizaos/cli": "latest",
    "@elizaos/core": "latest",
    "pm2": "^5.4.3",
    "socket.io-client": "^4.8.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}