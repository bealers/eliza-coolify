{
  "name": "elizify",
  "version": "1.0.0",
  "description": "Universal ElizaOS deployment platform - Deploy AI agents anywhere",
  "main": "index.js",
  "scripts": {
    "start": "bun run --bun scripts/start.sh",
    "dev": "NODE_ENV=development bun run --bun scripts/start.sh",
    "stop": "bun run scripts/stop-elizaos.sh",
    "status": "bun run scripts/status-elizaos.sh",
    "pm2:start": "./scripts/start-elizaos.sh",
    "pm2:stop": "./scripts/stop-elizaos.sh",
    "pm2:status": "./scripts/status-elizaos.sh",
    "health": "bun scripts/healthcheck.js",
